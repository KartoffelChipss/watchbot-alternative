<nav>
    <a href="/admin" class="title">
        <img src="/assets/img/logo.png">
        <h1>Argus Admin Dashboard</h1>
    </a>
    <div class="username-box">
        <% if (user) {%>
            <a class="username" href="#" onclick="toggleSidenav()">
                <div style="display: flex; flex-direction: column;">
                    <span><%= user.global_name %><% if (user.discriminator && user.discriminator !== "0") {%> #<%= user.discriminator %> <%}%></span>
                    <div class="nav-money">
                        <img title="Argus Points" src="/assets/img/token.png">
                        <span><%= userDoc.points.toLocaleString('en-US').replaceAll(',', ' ') %></span>
                    </div>
                </div>
                <%- user.avatar ? `<img src="https://cdn.discordapp.com/avatars/${user.id}/${user.avatar}.png">` : `<img src="/img/nopp_icon.svg">` %>
            </a>
        <%} else {%>
            <a href="/login">Login</a>
        <%}%>
    </div>
</nav>
